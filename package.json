{
  "name": "clean-node-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "start:dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/index.ts' -e ts",
    "husky:prepare": "husky install",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint 'src/**' --fix",
    "format:check": "prettier --check 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "vitest --passWithNoTests",
    "test:staged": "yarn test --run",
    "test:cov": "vitest run --coverage",
    "style:check": "prettier --check './src/**/*.ts'",
    "style:fix": "prettier --write './src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-lwazevedo:lwazevedo/clean-node-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitest/coverage-c8": "^0.25.8",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "git-commit-msg-linter": "^4.4.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4",
    "vitest": "^0.25.8"
  }
}
