{
  "name": "clean-node-ts",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node dist/main",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/main' -e ts",
    "clean": "rimraf dist",
    "build": "yarn clean && tsc -p tsconfig-build.json && tsc-alias",
    "lint": "eslint ./src  --ext .ts",
    "lint:fix": "eslint ./src  --ext .ts --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests src/**/*.ts",
    "test:push": "yarn test --coverage",
    "husky:prepare": "husky install",
    "format": "prettier --write 'src/**/*.ts'",
    "format:ck": "prettier --check 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-lwazevedo:lwazevedo/clean-node-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/nodemon": "^1.19.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "git-commit-msg-linter": "^4.4.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "rimraf": "^3.0.2"
  }
}
